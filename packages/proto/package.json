{
  "name": "@repo/proto",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "exports": {
    "./*": "./dist/*.js"
  },
  "scripts": {
    "build": "npm run generate && tsc",
    "generate": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/generated --grpc_out=grpc_js:./src/generated --ts_out=./src/generated --proto_path=./src ./src/*.proto && perl -0777 -i -pe 's/return super\\.([A-Za-z0-9_]+)\\(/return super.$1!\\(/g' src/generated/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/google-protobuf": "^3.15.12",
    "eslint": "^9.39.1",
    "typescript": "5.9.2"
  }
}
